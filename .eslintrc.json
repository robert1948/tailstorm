{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "camelcase": "error",
    "quotes": ["error", "single"],
    "indent": ["error", 2],
    "prettier/prettier": "error"
  }
}